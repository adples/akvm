{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"slug": "background",
					"color": "#ffffff",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#040707",
					"name": "Foreground"
				},
				{
					"slug": "primary",
					"color": "#F26322",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#434343",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#ED4A16",
					"name": "Tertiary"
				},
				{
					"slug": "primary-dark",
					"color": "#b63a00",
					"name": "Primary Dark"
				},
				{
					"slug": "primary-light",
					"color": "#f4773f",
					"name": "Primary Light"
				},
				{
					"slug": "light-grey",
					"color": "#f2f2f2",
					"name": "Light Grey"
				},
				{
					"slug": "neutral",
					"color": "#ddd",
					"name": "Neutral"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				}
			],
			"customGradient": true,
			"gradients": [
				{
					"name": "Default Gradient",
					"slug": "default-gradient",
					"gradient": "linear-gradient(to top in oklab, rgb(182, 58, 0) 0%, rgb(242, 99, 34) 100%)"
				},
				{
					"name": "Dark Gradient",
					"slug": "dark-gradient",
					"gradient": "linear-gradient(to top, rgba(0, 0, 0, 1) 0%, #040707 100%)"
				}
			]
		},
		"layout": {
			"contentSize": "100%",
			"wideSize": "100%"
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"fluid": true,
			"defaultFontSizes": false,
			"fontSizes": [
				{
					"slug": "reduce",
					"size": ".875rem",
					"name": "Reduce",
					"fluid": false
				},
				{
					"slug": "slight-reduce",
					"size": ".925rem",
					"name": "Slight Reduce",
					"fluid": false
				},
				{
					"slug": "heading-1",
					"size": "3.75rem",
					"name": "Heading 1",
					"fluid": false
				},
				{
					"slug": "heading-2",
					"size": "3rem",
					"name": "Heading 2",
					"fluid": false
				},
				{
					"slug": "heading-3",
					"size": "2.25rem",
					"name": "Heading 3",
					"fluid": false
				},
				{
					"slug": "heading-4",
					"size": "1.875rem",
					"name": "Heading 4",
					"fluid": false
				},
				{
					"slug": "heading-5",
					"name": "Heading 5",
					"size": "1.5rem",
					"fluid": false
				},
				{
					"slug": "heading-6",
					"name": "Heading 6",
					"size": "1.25rem",
					"fluid": false
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "'Montserrat', sans-serif",
					"slug": "montserrat",
					"name": "Montserrat",
					"fontFace": [
						{
							"fontFamily": "'Montserrat', sans-serif",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontWeight": "400 800",
							"src": [ "file:./fonts/Montserrat/Montserrat-VariableFont_wght.ttf" ]
						},
						{
							"fontFamily": "'Montserrat', sans-serif",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"fontWeight": "normal",
							"src": [ "file:./fonts/Montserrat/Montserrat-Italic-VariableFont_wght.ttf" ]
						}
					]
				},
				{
					"fontFamily": "'Teko', sans-serif",
					"slug": "teko",
					"name": "Teko",
					"fontFace": [
						{
							"fontFamily": "'Teko', sans-serif",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontWeight": "500 900",
							"src": [ "file:./fonts/Teko/Teko-VariableFont_wght.ttf" ]
						}
					]
				}
			]
		},
		"border": {
			"color": true,
			"radius": true
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "'Montserrat', sans-serif"
		}
	}
}
